script(src="public/js/edithall_editor.js")

mixin chair
	svg.chair(width="32", height="32")
		rect(x="2", y="2", width="28", height="10")
		rect(x="2", y="11", width="6", height="18")
		rect(x="24", y="11", width="6", height="18")
		rect(x="8", y="12", width="16", height="16")
		

mixin test
	label moi

div.he_container
	table.he_map
		- var y = 0;
		while y < 20
			tr
				- var x = 0;
				while x < 16
					td
						+chair
					- x++;
			- y++;